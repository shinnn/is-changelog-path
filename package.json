{
  "name": "is-changelog-path",
  "version": "1.0.0",
  "description": "Check if a file path is a CHANGELOG file",
  "repository": "shinnn/is-changelog-path",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "pretest": "eslint --config @shinnn/node index.js test.js",
    "test": "tap --coverage --reporter=spec test.js",
    "test-only": "tap --reporter=spec test.js"
  },
  "license": "MIT",
  "files": [
    "index.js"
