{
  "name": "is-changelog-path",
  "version": "1.1.0",
  "description": "Check if a given string looks like a CHANGELOG file path",
  "repository": "shinnn/is-changelog-path",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "pretest": "eslint --fix --format=codeframe index.js test.js",
    "test": "npm run-script test-only --silent -- --coverage",
    "test-only": "tap test.js"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
