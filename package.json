  "devDependencies": {
    "@shinnn/eslint-config-node": "^3.0.0",
    "eslint": "^3.13.0",
    "nyc": "^10.0.0",
    "tap": "^9.0.2"
  },
  "eslintConfig": {
    "extends": "@shinnn/node"
  }
